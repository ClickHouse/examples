{
    "index_patterns": [
      "${INDEX_NAME}"
    ],
    "data_stream": {},
    "template": {
      "settings": {
        "index": {
          "lifecycle": {
            "name": "filebeat"
          },
          "routing": {
            "allocation": {
              "include": {
                "_tier_preference": "data_hot"
              }
            }
          },
          "mapping": {
            "total_fields": {
              "limit": "10000"
            }
          },
          "refresh_interval": "30s",
          "number_of_shards": "1",
          "max_docvalue_fields_search": "200",
          "sort": {
            "field": [
              "kind",
              "commit.operation",
              "commit.collection",
              "time_us"
            ],
            "order": [
              "asc",
              "asc",
              "asc",
              "asc"
            ]
          },
          "number_of_replicas": "0"
        }
      },
      "mappings": {
        "_source": {
          "enabled": false
        },
        "dynamic_templates": [
          {
            "strings_as_keyword": {
              "match_mapping_type": "string",
              "mapping": {
                "ignore_above": 1024,
                "type": "keyword"
              }
            }
          }
        ],
        "properties": {
          "kind": {
            "type": "keyword"
          },
          "commit": {
            "properties": {
              "collection": {
                "type": "keyword"
              },
              "operation": {
                "type": "keyword"
              }
            }
          },
          "time_us": {
            "type": "date"
          }
        }
      },
      "aliases": {}
    }
  }
  